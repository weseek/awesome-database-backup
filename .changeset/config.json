{
  "$schema": "https://unpkg.com/@changesets/config@2.0.0/schema.json",
  "changelog": ["@awesome-backup/custom-changelog", {
    "owner": "ryu-sato",
    "repo": "awesome-backup",
    "noThanksUsers": ["dependabot[bot]"],
    "categories": [
      {
        "text": "BREAKING CHANGES",
        "pullRequestLabels": [
          "breaking"
        ]
      },
      {
        "text": "ğŸ’ Features",
        "changesetSummaryPatterns": [
          "/^feat/i"
        ],
        "pullRequestLabels": [
          "feature"
        ]
      },
      {
        "text": "ğŸš€ Improvement",
        "changesetSummaryPatterns": [
          "/^imprv/i"
        ],
        "pullRequestLabels": [
          "improvement"
        ]
      },
      {
        "text": "ğŸ› Bug Fixes",
        "changesetSummaryPatterns": [
          "/^fix/i",
          "/^hotfix/i"
        ],
        "pullRequestLabels": [
          "bug"
        ]
      },
      {
        "text": "ğŸ§° Maintenance",
        "changesetSummaryPatterns": [
          "/^support/i",
          "/^chore/i",
          "/^ci/i",
          "/^docs/i",
          "/^test/i"
        ],
        "pullRequestLabels": [
          "support",
          "dependencies"
        ]
      }
    ]
  }],
  "commit": false,
  "fixed": [],
  "linked": [],
  "access": "restricted",
  "baseBranch": "master",
  "updateInternalDependencies": "patch",
  "ignore": []
}
