{
  "tasks": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ],
      "env": ["!*"]
    },
    "test": {
      "cache": true,
      "dependsOn": [],
      "outputs": [
        "coverage/**",
        "vitest-ctrf/**"
      ],
      "env": ["*"]
    },
    "lint": {
      "cache": false,
      "outputs": [],
      "env": ["*"]
    },
    "test:performance:stream_mode": {
      "cache": false,
      "dependsOn": [
        "build"
      ],
      "outputs": [],
      "env": ["*"]
    },
    "test:performance:tempfile_mode": {
      "cache": false,
      "dependsOn": [
        "build"
      ],
      "outputs": [],
      "env": ["*"]
    }
  }
}
