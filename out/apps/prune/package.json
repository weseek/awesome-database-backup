{
  "name": "@awesome-backup/prune",
  "version": "0.0.1",
  "description": "Prune backuped files from Amazon S3 or Google Cloud Storage. You can set a custom S3 endpoint to use S3 based services like DigitalOcean Spaces instead of Amazon S3.",
  "author": "Ryu Sato <ryu@weseek.co.jp>",
  "license": "MIT",
  "homepage": "https://github.com/ryu-sato/awesome-backup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryu-sato/awesome-backup.git"
  },
  "bugs": {
    "url": "https://github.com/ryu-sato/awesome-backup/issues"
  },
  "directories": {
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "prune": "dist/apps/prune/src/bin/prune.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "// for development": "",
    "ts-node-dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register",
    "//// Cannot run in parallel due to shared S3, GCS mock": "",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests -i",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@awesome-backup/tsconfig": "^0.0.1",
    "@awesome-backup/core": "^0.0.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "node-schedule": "^2.1.0",
    "tmp": "^0.2.1",
    "bunyan": "^1.8.15",
    "universal-bunyan": "^0.9.2",
    "browser-bunyan": "^1.6.3"
  },
  "devDependencies": {
    "tsconfig-paths": "^3.12.0",
    "@types/bunyan": "^1.8.8",
    "@aws-sdk/client-s3": "^3.45.0",
    "@google-cloud/storage": "^5.16.1",
    "mongodb": "^4.4.1"
  },
  "engines": {
    "node": ">= 16.13.1 < 17",
    "yarn": ">= 1.22.4"
  }
}
