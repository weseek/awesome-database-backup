{
  "name": "@awesome-backup/list",
  "version": "0.0.4",
  "description": "List files from Amazon S3 or Google Cloud Storage. You can set a custom S3 endpoint to use S3 based services like DigitalOcean Spaces instead of Amazon S3.",
  "author": "Ryu Sato <ryu@weseek.co.jp>",
  "license": "MIT",
  "homepage": "https://github.com/ryu-sato/awesome-backup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryu-sato/awesome-backup.git"
  },
  "bugs": {
    "url": "https://github.com/ryu-sato/awesome-backup/issues"
  },
  "directories": {
    "test": "__tests__"
  },
  "files": ["dist"],
  "main": "dist/apps/list/src/list.js",
  "bin": {
    "list": "bin/list.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "// for development": "",
    "ts-node-dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register",
    "//// Cannot run in parallel due to shared S3, GCS mock": "",
    "test": "cross-env NODE_ENV=test jest --testTimeout 20000 --passWithNoTests -i",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@awesome-backup/commands": "^0.0.4",
    "bunyan": "^1.8.15",
    "universal-bunyan": "^0.9.2",
    "browser-bunyan": "^1.6.3"
  },
  "devDependencies": {
    "@awesome-backup/tsconfig": "^0.0.4",
    "@awesome-backup/storage-service-test": "^0.0.4",
    "cross-env": "^7.0.3",
    "tsconfig-paths": "^4.0.0",
    "@types/bunyan": "^1.8.8"
  },
  "engines": {
    "node": ">= 16.13.1 < 17",
    "yarn": ">= 1.22.4"
  }
}
